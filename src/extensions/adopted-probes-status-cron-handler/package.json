{
	"name": "directus-extension-adopted-probes-status-cron-handler",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-operation"
	],
	"type": "module",
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.ts",
			"api": "src/api.ts"
		},
		"host": "^10.2.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"test": "TS_NODE_TRANSPILE_ONLY=true mocha"
	},
	"devDependencies": {
		"@directus/extensions": "^0.2.0",
		"@directus/extensions-sdk": "10.2.0",
		"@types/chai": "^4.3.11",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.10.4",
		"@types/sinon": "^17.0.2",
		"chai": "^4.3.10",
		"mocha": "^10.2.0",
		"nock": "^13.4.0",
		"sinon": "^17.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"vue": "^3.3.11"
	},
	"dependencies": {
		"axios": "^1.6.2",
		"lodash": "^4.17.21"
	}
}
