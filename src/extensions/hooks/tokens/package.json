{
	"name": "directus-extension-tokens",
	"description": "This extension hides tokens values from user",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook"
	],
	"type": "module",
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.1.1"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"test": "echo 'No tests for that extension'"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.1.1",
		"@types/node": "^20.2.3",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"@directus/errors": "^0.0.2",
		"joi": "^17.12.0",
		"lodash": "^4.17.21"
	}
}
