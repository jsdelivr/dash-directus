		"@types/node": "^20.9.0",
		"@types/sinon": "^17.0.1",
		"chai": "^4.3.10",
		"mocha": "^10.2.0",
		"nock": "^13.3.8",
		"sinon": "^17.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"axios": "^1.6.1"
