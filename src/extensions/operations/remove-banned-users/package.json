{
	"name": "directus-extension-remove-banned-users",
	"description": "Handle banned users CRON job. Reads directus users, users not found on github are deleted.",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-operation"
	],
	"type": "module",
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.ts",
			"api": "src/api.ts"
		},
		"host": "^10.3.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"test": "TS_NODE_TRANSPILE_ONLY=true mocha"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.3.0",
		"@types/chai": "^4.3.11",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.11.2",
		"@types/sinon": "^17.0.3",
		"chai": "^5.0.0",
		"mocha": "^10.2.0",
		"sinon": "^17.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"vue": "^3.4.14"
	},
	"dependencies": {
		"axios": "^1.6.5",
		"bluebird": "^3.7.2"
	}
}
